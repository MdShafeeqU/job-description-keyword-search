{
    "manifest_version": 2,
    "name": "Skill Search",
    "version": "0.1.0",
    "background": {
        "persistent": false,
        "scripts": [ "src/script.js" ]
     },
    "permissions": [ 
      "contextMenus",
      "http://localhost:8080/" 
     ],
     "externally_connectable": {
      "matches": ["<all_urls>"]
  }
  }